make -j8 (faster make)

make spotless (cleans)

cp -r /afs/cats/courses/cse111-wm/Assignments/asg1-dc-bigint/code/ . (copy over fold for assignments)

gedit ()

gdb lines
	add -g to your compiler line
	use gdb ./program_name to run
	type run
	use backtrace or bt to see stack
	use frame (num) to go to different methods
	use finish to finish each method
	hold control then X then A to see code in vi
	use p (variable_name) to see variable values
	use n to go to next line
	quit to get out of gdb